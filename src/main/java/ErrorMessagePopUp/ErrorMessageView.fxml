<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="150.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ErrorMessagePopUp.ErrorMessageController">
   <children>
      <VBox alignment="CENTER" spacing="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label fx:id="messageLabel2" text="Link" />
            <Label fx:id="messageLabel" text="Message" />
            <HBox alignment="BOTTOM_CENTER" spacing="10.0">
               <children>
                  <Button fx:id="noButton" mnemonicParsing="false" onAction="#handleNoButton" prefHeight="25.0" prefWidth="70.0" text="Nein" />
                  <Button fx:id="yesButton" mnemonicParsing="false" onAction="#handleYesButton" prefHeight="25.0" prefWidth="70.0" text="Ja" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
