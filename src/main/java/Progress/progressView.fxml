<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Progress.progressController">
   <children>
      <VBox alignment="CENTER" layoutX="150.0" layoutY="-19.0" prefHeight="119.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
         <children>
            <StackPane>
               <children>
                  <Label fx:id="doneLabel" text="Alle Fotos wurden konvertiert." textFill="#409714">
                     <font>
                        <Font size="18.0" />
                     </font></Label>
                  <Label fx:id="toDoLabel" text="20 Fotos werden konvertiert." textFill="RED">
                     <font>
                        <Font size="18.0" />
                     </font></Label>
               </children>
            </StackPane>
            <HBox alignment="CENTER" spacing="1.0" />
            <HBox alignment="CENTER" spacing="10.0">
               <children>
                  <Button fx:id="finishButton" mnemonicParsing="false" onAction="#handleFinishButton" text="Beenden" />
               </children>
               <VBox.margin>
                  <Insets left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
